{
    "name": "react-ssr",
    "version": "1.0.0",
    "description": "Server Side Rendering React App",
    "main": "src/index.js",
    "author": "Amir M.",
    "license": "MIT",
    "scripts": {
        "dev": "npm-run-all --parallel dev:*",
        "dev:run-server": "nodemon --watch build --exec 'node build/bundle.js'",
        "dev:build-server": "webpack --config webpack.server.js --mode=development --watch",
        "dev:build-client": "webpack --config webpack.client.js --mode=development --watch"
    },
    "dependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-function-bind": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
        "@babel/plugin-transform-regenerator": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@reduxjs/toolkit": "^1.2.5",
        "axios": "^0.19.2",
        "babel-loader": "^8.0.6",
        "classnames": "^2.2.6",
        "express": "^4.17.1",
        "express-http-proxy": "^1.6.0",
        "nodemon": "^2.0.2",
        "npm-run-all": "^4.1.5",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-helmet": "^5.2.1",
        "react-helmet-async": "^1.0.4",
        "react-redux": "^7.1.3",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.5",
        "regenerator-runtime": "^0.13.3",
        "serialize-javascript": "^3.0.0",
        "webpack": "^4.41.6",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
